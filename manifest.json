{
    "manifest_version": 2,
    "name": "OnClick Translator",
    "version": "1.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
      ],
    "browser_action": {
        "default_title": "Test",
        "default_popup": "popup.html"
    }
}